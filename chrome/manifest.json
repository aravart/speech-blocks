{
    "manifest_version": 2,
    "name": "SpeechBlocks Injector",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["https://blockly-games.appspot.com/*"],
            "js": ["chrome.js"]
        }
    ],
    "web_accessible_resources": [
        "speech-blocks/*",
        "init.js"
    ]
}
